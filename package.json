{
  "name": "ultramarket-platform",
  "version": "3.0.0",
  "description": "ðŸš€ UltraMarket E-commerce Platform - Professional Enterprise Solution",
  "main": "src/main.ts",
  "scripts": {
    "start": "npx ts-node start.ts",
    "dev": "npx ts-node start.ts",
    "start:minimal": "npx ts-node start.ts minimal",
    "start:business": "npx ts-node start.ts business",
    "start:full": "npx ts-node start.ts full",
    "build": "npm run build:libs && npm run build:backend && npm run build:frontend",
    "build:libs": "tsc -p packages/shared/tsconfig.json",
    "build:backend": "npm run build:microservices",
    "build:frontend": "npm run build:web && npm run build:admin",
    "build:microservices": "concurrently \"npm run build:auth\" \"npm run build:products\" \"npm run build:orders\" \"npm run build:payments\" \"npm run build:gateway\"",
    "build:auth": "tsc -p backend/core/auth-service/tsconfig.json",
    "build:products": "tsc -p backend/business/product-service/tsconfig.json",
    "build:orders": "tsc -p backend/business/order-service/tsconfig.json",
    "build:payments": "tsc -p backend/business/payment-service/tsconfig.json",
    "build:gateway": "tsc -p backend/core/api-gateway/tsconfig.json",
    "build:web": "cd frontend/web-app && npm run build",
    "build:admin": "cd frontend/admin-panel && npm run build",
    "start:databases": "node scripts/start-databases.js",
    "start:backend": "concurrently \"npm run start:auth\" \"npm run start:products\" \"npm run start:orders\" \"npm run start:payments\" \"npm run start:gateway\"",
    "start:frontend": "concurrently \"npm run start:web\" \"npm run start:admin\"",
    "start:auth": "node backend/core/auth-service/dist/main.js",
    "start:products": "node backend/business/product-service/dist/main.js",
    "start:orders": "node backend/business/order-service/dist/main.js",
    "start:payments": "node backend/business/payment-service/dist/main.js",
    "start:gateway": "node backend/core/api-gateway/dist/main.js",
    "start:web": "cd frontend/web-app && npm run dev",
    "start:admin": "cd frontend/admin-panel && npm run dev",
    "dev:databases": "node scripts/start-databases.js",
    "dev:backend": "concurrently \"npm run dev:auth\" \"npm run dev:products\" \"npm run dev:orders\" \"npm run dev:payments\" \"npm run dev:gateway\"",
    "dev:frontend": "concurrently \"npm run dev:web\" \"npm run dev:admin\"",
    "dev:auth": "ts-node backend/core/auth-service/src/main.ts",
    "dev:products": "ts-node backend/business/product-service/src/main.ts",
    "dev:orders": "ts-node backend/business/order-service/src/main.ts",
    "dev:payments": "ts-node backend/business/payment-service/src/main.ts",
    "dev:gateway": "ts-node backend/core/api-gateway/src/main.ts",
    "dev:web": "cd frontend/web-app && npm run dev",
    "dev:admin": "cd frontend/admin-panel && npm run dev",
    "setup": "npm run setup:deps && npm run setup:dbs && npm run setup:build",
    "setup:deps": "npm install && npm run install:frontend",
    "setup:dbs": "npm run setup:databases && npm run migrate",
    "setup:build": "npm run build",
    "setup:databases": "node scripts/setup-databases.js",
    "migrate": "npx prisma generate && npx prisma db push",
    "install:frontend": "cd frontend/web-app && npm install && cd ../admin-panel && npm install && cd ../..",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "jest --config=jest.config.js",
    "test:frontend": "cd frontend/web-app && npm test && cd ../admin-panel && npm test && cd ../..",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "health": "curl http://localhost:3000/health && curl http://localhost:3001/health",
    "logs": "tail -f logs/*.log",
    "stop": "pkill -f \"node.*ultramarket\" && pkill -f \"ts-node.*ultramarket\"",
    "restart": "npm run stop && sleep 3 && npm start",
    "deploy": "npm run build && npm run start"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^9.0.3",
    "@prisma/client": "^5.7.1",
    "@sentry/node": "^9.42.0",
    "@testing-library/react": "^16.3.0",
    "@types/mongoose": "^5.11.96",
    "@vitejs/plugin-react": "^4.7.0",
    "axios": "^1.6.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.12.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dataloader": "^2.2.2",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "express-rate-limit": "^8.0.1",
    "handlebars": "^4.7.8",
    "helmet": "^7.2.0",
    "isomorphic-dompurify": "^2.26.0",
    "joi": "^17.12.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "minio": "^8.0.5",
    "moment": "^2.29.4",
    "mongodb": "^6.3.0",
    "mongoose": "^8.16.5",
    "morgan": "^1.10.0",
    "msw": "^2.10.4",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "pg": "^8.11.3",
    "pino": "^8.17.1",
    "pino-pretty": "^10.3.1",
    "prisma": "^5.7.1",
    "qrcode": "^1.5.3",
    "redis": "^4.6.12",
    "socket.io": "^4.7.4",
    "speakeasy": "^2.0.0",
    "uuid": "^9.0.1",
    "vite": "^6.3.5",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "zod": "^4.0.10"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/crypto-js": "^4.2.1",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsdom": "^21.1.7",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.202",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
    "@types/nodemailer": "^6.4.14",
    "@types/pg": "^8.10.9",
    "@types/qrcode": "^1.5.5",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.7",
    "@types/validator": "^13.15.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ultramarket/platform"
  },
  "keywords": [
    "e-commerce",
    "microservices",
    "typescript",
    "react",
    "node.js",
    "professional",
    "enterprise",
    "ultramarket"
  ],
  "author": "UltraMarket Development Team",
  "license": "PROPRIETARY",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
